#include <stdio.h>

int main() {
    int a[4] = {2, 3, 1, 8};
    int n = 4;

    for (int i = 0; i < (1 << n); i++) {
        printf("{ ");
        
        for (int j = 0; j < n; j++) {
            if (i & (1 << j)) {
                printf("%d ", a[j]);
            }
        }
        
        printf("}\n");
    }

    return 0;
}
