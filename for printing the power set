#include <stdio.h>

int main() {
// print the power set
    int a[3] = {10, 12, 15};
         
    printf("{}\n");

    for (int i = 0; i < 3; i++) {
        printf("%d\n", a[i]);
        
        for (int j = i+1; j < 3; j++) {
            printf("%d, %d\n", a[i], a[j]);
    
        }
    }

    return 0;
}
